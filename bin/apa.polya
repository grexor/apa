#!/usr/bin/python

import apa
import sys

import argparse
parser = argparse.ArgumentParser()
parser.add_argument('-poly', action="store", dest="poly", default=None)
args = parser.parse_args()

if args.poly==None:
    print "provide -poly"
    sys.exit(1)

apa.polya.process(args.poly)
apa.polya.pas_db(args.poly)
