#!/usr/bin/python

import apa
import sys

import argparse
parser = argparse.ArgumentParser()
parser.add_argument('-poly_id', action="store", dest="poly_id", default=None)
args = parser.parse_args()

if args.poly_id==None:
    print "Provide -poly_id"
    sys.exit(1)
    
apa.polya.process(args.poly_id)
